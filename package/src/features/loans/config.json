{
    "headerTitle": "Loans",
    "components": [
        {
            "id": "headertitle",
            "type": "headertitle"
        },
        {
            "id": "searchbarwithfilter",
            "type": "searchbarwithfilter"
        },
        {
            "id": "loanlist",
            "type": "loanlist"
        }
    ],
    "filters": [
        {
            "key": "barangay",
            "type": "dropdown",
            "options": [
                {
                    "id": 1,
                    "title": "Ampayon",
                    "value": "ampayon"
                },
                {
                    "id": 2,
                    "title": "San Vicente",
                    "value": "san_vicente"
                },
                {
                    "id": 3,
                    "title": "Poblacion",
                    "value": "poblacion"
                }
            ],
            "default": "libertad",
            "disabled": true,
            "label": "Barangay",
            "preserveOnClear": true
        },
        {
            "key": "purok",
            "type": "dropdown",
            "options": [
                {
                    "id": 0,
                    "title": "All",
                    "value": ""
                },
                {
                    "id": 1,
                    "title": "Purok 1",
                    "value": "purok_1"
                },
                {
                    "id": 2,
                    "title": "Purok 2",
                    "value": "purok_2"
                },
                {
                    "id": 3,
                    "title": "Purok 3",
                    "value": "purok_3"
                }
            ],
            "default": "",
            "disabled": false,
            "label": "Purok",
            "preserveOnClear": false
        },
        {
            "key": "address",
            "type": "text",
            "placeholder": "Enter address...",
            "default": "",
            "disabled": false,
            "label": "Address",
            "preserveOnClear": false
        },
        {
            "key": "householdHead",
            "type": "range",
            "min": 0,
            "max": 50,
            "default": [
                0,
                50
            ],
            "disabled": false,
            "label": "Household Head",
            "preserveOnClear": false
        }
    ]
}